\ql \li0\ri0\widctlpar\intbl\aspalpha\aspnum\faauto\adjustright\rin0\lin0 {\fs20\cf0 \trowd \trgaph108\trrh830\trleft-108\trbrdrh\brdrs\brdrw10 \trftsWidth3\trwWidth10134\trpaddl108\trpaddr108\trpaddfl3\trpaddfr3 \clvertalt\clbrdrt\brdrs\brdrw10 \clbrdrb
